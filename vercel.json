{
  "name": "comment",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "dashboard.js",
      "use": "@vercel/node@3.0.12"
    },
    {
      "src": "index.js",
      "use": "@vercel/node@3.0.12"
    }
  ],
  "rewrites": [
    {
      "source": "/ui(.*)",
      "destination": "dashboard.js"
    },
    {
      "source": "/(.*)",
      "destination": "index.js"
    }
  ]
}
