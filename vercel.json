{
  "name": "comment",
  "github": {
    "silent": true
  },
  "functions": {
    "dashboard.js": {
      "runtime": "@vercel/node@1"
    },
    "index.cjs": {
      "runtime": "@vercel/node@1"
    }
  },
  "builds": [
    {
      "src": "robots.txt",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/ui(.*)",
      "destination": "/dashboard.js"
    },
    {
      "source": "/((?!robots\\.txt$).*)",
      "destination": "index.cjs"
    }
  ]
}
