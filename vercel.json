{
  "name": "comment",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "dashboard.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "robots.txt",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/ui(.*)",
      "destination": "/dashboard.js"
    },
    {
      "source": "/((?!robots\\.txt$).*)",
      "destination": "index.cjs"
    }
  ]
}
